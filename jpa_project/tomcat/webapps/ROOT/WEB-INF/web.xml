<web-app>
<welcome-file-list>
   <welcome-file>index.jsp</welcome-file>
</welcome-file-list>

<servlet>
   <servlet-name>signUpProj1</servlet-name>
   <servlet-class>Register</servlet-class>
</servlet>

<servlet>
   <servlet-name>signUpProj2</servlet-name>
   <servlet-class>RegisterJPA</servlet-class>
</servlet>

<servlet>
   <servlet-name>LoginProject1</servlet-name>
   <servlet-class>Login</servlet-class>
</servlet>

<servlet>
   <servlet-name>LoginProject2</servlet-name>
   <servlet-class>LoginJPA</servlet-class>
</servlet>


<servlet-mapping>
   <servlet-name>signUpProj1</servlet-name>
   <url-pattern>/signUpProject1</url-pattern>
</servlet-mapping>

<servlet-mapping>
   <servlet-name>signUpProj2</servlet-name>
   <url-pattern>/signUpProject2</url-pattern>
</servlet-mapping>

<servlet-mapping>
   <servlet-name>LoginProject1</servlet-name>
   <url-pattern>/loginProject1</url-pattern>
</servlet-mapping>


<servlet-mapping>
   <servlet-name>LoginProject2</servlet-name>
   <url-pattern>/loginProject2</url-pattern>
</servlet-mapping>

</web-app>








